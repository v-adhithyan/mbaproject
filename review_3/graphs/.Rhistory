require(ggplot2)
require(stringr)
data <- read.csv("data/price.csv")
png(file_name="all_in_one.png")
g <- ggplot(data, aes(id))
g <- g + geom_line(aes(y=sbm, color="blue"))
g <- g + geom_line(aes(y=sbt, color="green"))
g <- g + geom_line(aes(y=sbi, color="yellow"))
g <- g + geom_line(aes(y=sbbj, color="magenta"))
g <- g + scale_color_discrete(name="Banks", labels=c("State Bank of Mysore",
"State Bank of Travancore",
"State Bank of India",
"State Bank of Bikaner and Jaipur"))
g <- g + labs(title = str_to_upper("Share price of SBI and it's associates during merger period"))
g <- g + scale_x_discrete(name="Year", breaks = c(1, 247),  limits = 1:250, labels = c("2016", "2017"))
g <- g + scale_y_continuous(name="INR")
ggsave(filename="plots/all_in_one.png")
g
